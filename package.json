  "description": "JavaScript WebGL 3D maps rendering engine",
  "main": "src/browser/index.js",
  "scripts": {
    "install": "",
    "clean": "node_modules/.bin/rimraf build test/perf/results/ dist/",
    "dist": "NODE_ENV=production node_modules/.bin/webpack --progress",
    "start": "node_modules/.bin/webpack-dev-server --content-base ./ --inline",
    "prepare": "husky install",
    "perf:install": "npx playwright install chromium",
    "test": "test:perf",
    "test:perf": "node ./test/perf/run-bundle.js",
    "test:perf:headed": "PWDEBUG=console node ./test/perf/run-bundle.js",
    "test:perf:debug": "PWDEBUG=1 node ./test/perf/run-bundle.js",
    "test:perf:one": "node ./test/perf/run-one.js",
    "test:perf:one:headed": "PWDEBUG=console node ./test/perf/run-one.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melown/vts-browser-js.git"
  },
  "keywords": [
    "Web",
