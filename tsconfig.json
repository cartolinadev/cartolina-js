{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es2020",
    "lib": ["es2020", "dom"],
    "module": "es2020",
    "allowJs": true,
    "checkJs": false,    
    "experimentalDecorators": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
    "moduleResolution": "bundler"
  },
  "include": ["src/**/*"],
  "exclude": ["src/core/map/geodata-processor/**"]
}
